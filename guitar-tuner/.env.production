# Production Environment Configuration

# Build optimizations
VITE_NODE_ENV=production
VITE_BUILD_TARGET=es2022

# Security settings
VITE_CSP_ENABLED=true
VITE_HTTPS_REQUIRED=true

# Performance settings
VITE_ENABLE_COMPRESSION=true
VITE_ENABLE_CACHE=true

